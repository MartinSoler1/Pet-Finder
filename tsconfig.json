{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "api/hello/route.js",
    "src/pages/api/hello/route.js",
    "src/pages/api/auth/[...nextauth]/index.js",
    "src/pages/api/auth/[...nextauth]/index.js"
  ],
  "exclude": ["node_modules"]
}
